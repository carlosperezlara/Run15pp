Log            = log/_$(Cluster).log
Output         = log/_$(ID).out
Error          = log/_$(ID).err
Requirements   = (CPU_Experiment == "phenix") && (MEMORY_LIMIT > 2000000) && (CPU_Speed >= 1)

Priority       = +20
GetEnv         = True
+Experiment    = "PHENIX"
+Job_Type      = "highmem"

Queue $(N)
