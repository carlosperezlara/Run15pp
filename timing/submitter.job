N = 107
ID = $(Cluster).$(Process) 

Universe       = vanilla
Initialdir     = /gpfs/mnt/gpfs02/phenix/mpcex/cperez/treeAnalysis/timing
Executable     = /bin/csh
Arguments      = dojob.csh $(Process)

